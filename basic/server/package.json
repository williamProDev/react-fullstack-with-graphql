{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "graphcool deploy && node src/index.js",
    "deploy": "now --public && now alias && now rm --yes --safe graphql-template-node"
  },
  "dependencies": {
    "graphcool-binding": "0.2.4",
    "graphql-import": "0.1.8",
    "graphql-yoga": "0.4.3"
  },
  "devDependencies": {
    "now": "8.5.4"
  },
  "now": {
    "alias": "graphql-template-node"
  }
}
